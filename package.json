  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "node": true
    }
  },
  "xo": {
    "space": true
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 120,
    "bracketSpacing": false
  },
  "lint-staged": {
    "linters": {
      "**/frontend/_public/**/*.js": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ]
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/scripts/tests/jest.setup.js",
      "<rootDir>/shopware/themes/Frontend/Responsive/frontend/_public/src/js/jquery.plugin-base.js",
      "<rootDir>/shopware/themes/Frontend/Responsive/frontend/_public/src/js/jquery.state-manager.js",
      "<rootDir>/shopware/themes/Frontend/Responsive/frontend/_public/src/js/jquery.storage-manager.js"
    ],
    "testPathIgnorePatterns": [
      "/shopware/tests/",
      "/node_modules/",
      "/vendor/"
    ],
    "moduleNameMapper": {
      "^Responsive/(.*)": "<rootDir>/shopware/themes/Frontend/Responsive/frontend/_public/src/js/$1",
      "^Bare/(.*)": "<rootDir>/shopware/themes/Frontend/Bare/frontend/_public/src/js/$1"
    }
  }
